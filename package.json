{
  "name": "whoop-cli",
  "version": "0.1.0",
  "description": "CLI for fetching WHOOP health data (sleep, recovery, HRV, strain) - designed for humans and AI agents",
  "keywords": [
    "whoop",
    "health",
    "fitness",
    "cli",
    "api",
    "sleep",
    "recovery",
    "hrv",
    "strain",
    "llm",
    "ai-agent"
  ],
  "author": "Siddhant Jain <post.siddhant@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siddhantjain/whoop-cli.git"
  },
  "bugs": {
    "url": "https://github.com/siddhantjain/whoop-cli/issues"
  },
  "homepage": "https://github.com/siddhantjain/whoop-cli#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "whoop": "./dist/cli.js"
  },
  "files": [
    "dist",
    "SKILL.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
